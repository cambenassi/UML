	.data
A:	.word 	5:20
prompt:	.asciiz	"Sum of array A is "	
	
	.text
	
	li	$s0, 0		#sum = 0
	li	$t0, 0		#i = 0
	
rept:
	sll	$t0, $t0, 2	#i*4
	lw 	$t1, A($t0)	#$t1 = A[i]
	sra	$t0, $t0, 2	#i / 4
	
	add 	$s0, $s0, $t1	#sum += A[i]
	addi	$t0, $t0, 1	#i++
	blt	$t0, 20, rept	#if (i<N) goto rept
	
	la	$a0, prompt
	li	$v0, 4
	syscall
	
	move	$a0, $s0
	li	$v0, 1
	syscall
	
	